msgid "%cDirty: %s, Writeback: %s, processes: %d"
msgstr "%cНе записано: %s, записывается: %s, процессов осталось: %d"

#: syncp.c:352
#, c-format
msgid "\n"
msgstr "\n"

#: syncp.c:356
msgid "timeout is exceeded, probably still syncing"
msgstr "таймаут превышен, возможно, всё ещё синхронизируемся"

#: syncp.c:361
msgid "can't sync some data"
msgstr "невозможно синхронизировать какие-то данные"
